{
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": false,
      "skipDefaultLibCheck": false,
      "lib": ["esnext", "dom"],
      "outDir": "./dist",
      "rootDir": "./src",
      "paths": {
        "@/*": ["./src/*"]
      }
    },
  }