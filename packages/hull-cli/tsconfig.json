{
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "declaration": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": ".",
    },
    "include": ["./src"],
    "exclude": ["./template"]
  }