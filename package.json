{
  "name": "hulljs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "release": "bash ./script/release.sh",
    "version": "standard-version",
    "start": "dumi dev",
    "build": "lerna run build",
    "build-doc": "dumi build",
    "bootstrap": "lerna bootstrap --hoist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -eGIT_classnamesPARAMS $"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint  --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoguoxiong/hulljs.git"
  },
  "author": "luoguoxiong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luoguoxiong/hulljs/issues"
  },
  "homepage": "https://github.com/luoguoxiong/hulljs#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "dumi": "^1.1.42",
    "husky": "^8.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "standard-version": "^9.5.0",
    "yorkie": "^2.0.0",
    "chalk": "^4.0.1",
    "escape-goat": "^3.0.0"
  },
  "__npminstall_done": false,
  "dependencies": {
    "@umijs/utils": "^3.5.26",
    "git-repo-info": "^2.1.1"
  }
}
