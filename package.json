  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "conventional-changelog-angular-bitbucket": "^1.2.0",
    "dumi": "^1.1.42",
    "husky": "^8.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
