{
  "name": "hulljs",
  "version": "0.0.3",
  "description": "hulljs",
  "main": "index.js",
  "scripts": {
    "version": "standard-version",
    "start": "dumi dev",
    "build": "lerna run build",
    "build-doc": "dumi build",
    "bootstrap": "lerna bootstrap --hoist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -eGIT_classnamesPARAMS $"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint  --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoguoxiong/hulljs.git"
  },
  "author": "luoguoxiong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luoguoxiong/hulljs/issues"
  },
  "homepage": "https://github.com/luoguoxiong/hulljs#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "dumi": "^1.1.42",
    "husky": "^8.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "standard-version": "^9.5.0",
    "yorkie": "^2.0.0"
  },
  "standard-version": {
    "issueUrlFormat": "https://xxx.com/{{id}}",
    "types": [
      {
        "type": "feat",
        "section": "需求"
      },
      {
        "type": "fix",
        "section": "Bug 修复"
      },
      {
        "type": "perf",
        "section": "优化"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "conflict",
        "hidden": true
      },
      {
        "type": "revert",
        "hidden": true
      }
    ]
  },
  "__npminstall_done": false
}
